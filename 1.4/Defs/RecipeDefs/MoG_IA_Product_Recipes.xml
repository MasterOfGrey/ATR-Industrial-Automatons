<Defs>

<!-- Make Steel Fibres into Other Things -->
	<!-- Make Yarn -->
	<RecipeDef>
		<defName>ATR_Make_S_FibresIntoYarn</defName>
		<label>Spin steel fibres into steel yarn</label>
		<description>Use special spinning tools to spin steel fibres into a wool-like yarn.</description>
		<jobString>Spinning steel yarn.</jobString>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<workAmount>1200</workAmount>
        <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>ATR_SteelFibres</li>
					</thingDefs>
				</filter>
				<count>240</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>ATR_SteelFibres</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<ATR_SteelYarn>15</ATR_SteelYarn>
		</products>
		<skillRequirements>
		  <Crafting>3</Crafting>
		</skillRequirements>
		<workSkill>Crafting</workSkill>
	    <recipeUsers>
			<li>ATR_ProductRefinementTable</li>
        </recipeUsers>
	</RecipeDef>
	
